@{
}
@model List<Employee>
<h2>Employee List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Employee Id</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.EmployeeId</td>
                <td>@employee.FirstName</td>
                <td>@employee.LastName</td>
                <td>@employee.Email</td>
                <td>@employee.Department</td>
                <td>
                    <a href="/Employee/Details/@employee.Id">Details</a> |



                </td>
            </tr>
        }
    </tbody>
</table>